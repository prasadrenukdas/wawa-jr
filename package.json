{
  "name": "rnts3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "test": "tsc --project tsconfig.test.json && tslint --project tsconfig.test.json && jest",
    "lint": "tslint --project tsconfig.test.json"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-native": "0.53.0",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-navigation": "^1.1.385",
    "react-native-snap-carousel": "^3.6.0",
    "react-navigation": "^1.5.7",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0-beta.2",
    "redux-observable": "^0.18.0",
    "rxjs": "^5.5.6"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write --parser typescript --single-quote --trailing-comma all",
      "tslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "@types/react": "^16.0.36",
    "@types/react-native": "^0.52.8",
    "@types/react-redux": "^5.0.15",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "babel-preset-react-native": "^4.0.0",
    "husky": "^0.14.3",
    "jest": "22.1.4",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.2.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "ts-config-mobiquity-react-native": "^0.0.17",
    "ts-jest": "^22.0.3",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/__tests__/.+\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "**/*.tsx",
      "**/*.ts"
    ],
    "moduleNameMapper": {
      "src(.*)": "<rootDir>/src/$1"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  }
}
